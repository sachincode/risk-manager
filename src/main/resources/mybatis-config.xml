<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="useGeneratedKeys" value="true" />
        <setting name="cacheEnabled" value="true" />
        <setting name="defaultExecutorType" value="REUSE" />
    </settings>

    <typeHandlers>
        <typeHandler handler="com.sachin.risk.common.core.mybatis.DataTypeHandler"
                     javaType="com.sachin.risk.common.core.enums.DataType" />
        <typeHandler handler="com.sachin.risk.common.core.mybatis.BusiTypeHandler"
                     javaType="com.sachin.risk.common.core.enums.BusiType" />
        <typeHandler handler="com.sachin.risk.common.core.mybatis.EncryptTypeHandler"
                     javaType="com.sachin.risk.common.core.enums.EncryptType" />
        <typeHandler handler="com.sachin.risk.common.core.mybatis.EventScopeHandler"
                     javaType="com.sachin.risk.common.core.enums.EventScope" />
    </typeHandlers>

</configuration>